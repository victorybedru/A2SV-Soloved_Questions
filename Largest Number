class Solution:
    def largestNumber(self, nums: List[int]) -> str:
        numList=[str(x) for x in nums]
        def compare(a,b):
            if a+b> b+a:
                return -1
            elif a+b< b+a:
                return 1
            else:
                return 0
        numList.sort(key=cmp_to_key(compare))
        res= "".join(numList)
        return("0" if res[0]=="0" else res)

